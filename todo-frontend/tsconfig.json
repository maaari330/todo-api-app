{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ES2020",
        "lib": [
            "DOM",
            "ES2020"
        ],
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "react",
            "react-dom",
            "node"
        ]
    }
}